<?xml version='1.0'?>
<aspect
    xmlns="http://www.crowdfusion.com/schema/aspect"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.crowdfusion.com/schema/aspect http://www.crowdfusion.com/schema/crowdfusion-aspect-2.0.xsd">

  <info>
    <name>WB Mixin: Plugger</name>
    <description>Given to elements that have one associated plugger. Plugger is a media spa application.</description>
    <elementmode>many</elementmode>
  </info>

  <meta_defs>
    <meta id="#plugger-id">
      <title>Plugger Id</title>
      <validation datatype="string" max="20"/>
    </meta>
  </meta_defs>

</aspect>